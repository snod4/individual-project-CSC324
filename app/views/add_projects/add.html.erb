
<h1>Add Project</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@added) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :date %>
      <%= f.text_field :date %>

      <%= f.label :description %>
      <%= f.text_area( :description, size: '50x10') %>

      <%= f.submit "Add Project", class: "add-button" %>
    <% end %>
  </div>
</div>
